#include <stdio.h>
#include <stdlib.h>

int binary_search(int v[], int inicio, int fim, int num) 
{
    
    if(inicio <= fim) 
    {   
        int meio;
        meio = (inicio + fim)/2;
        
        if(v[meio] < num)
            return binary_search(v, meio+1, fim, num);

        else if (v[meio] > num)
            return binary_search(v, inicio, meio - 1, num);
        
        int x = v[meio];
        
        while(v[meio] == x){
            meio--;
        }
        
        return ++meio;
        
    }return -1;
}

int main() 
{   
    int matriz_size, num_find, i, k, value_find;
    scanf("%d""%d", &matriz_size, &num_find);
    int vet[matriz_size];
    
    for(i = 0; i < matriz_size;  i++)
    {
        scanf("%d", &vet[i]);
    }
    
    int inicio = 0;
    int fim = sizeof(vet)/sizeof(int);
    
    for (k = 0; k < num_find;  k++)
    {
        scanf("%d", &value_find);
        printf("%d\n", binary_search(vet, inicio, fim, value_find));
        
    }
    
    return 0;
}
